#include<stdio.h>
#include<math.h>

int main ()
{

    int a, b, c;

    a = 217;
    b = 14;
    c = 6;

    if ((a > b)
        && (a > c)){

        printf(" %d eh o maior", a);
        }
    if ((b > a)
        && (b > c)){

        printf(" %d eh o maior", b);

    }if (( c > a) && ( c > b)){

        printf(" %d eh maior", c);
    }

    return 0;

}
