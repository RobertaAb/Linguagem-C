#include<stdio.h>
#include<math.h>

double main(){

    double A, B, C, PA, PB, PC, MEDIA, SOMAP;

    PA = 2;
    PB = 3;
    PC = 5;
    SOMAP = PA+PB+PC;
    A = 5.0 * PA;
    B = 6.0 * PB;
    C = 7.0 * PC;
    MEDIA = (A + B + C)/ SOMAP;

    printf("MEDIA = %.1f \n\n", MEDIA);
    return 0;
}
